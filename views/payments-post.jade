
block content
	.container(ng-controller="settingsCtrl")
		.jumbotron
			.jumbotronHeader.clearfix
					.title
						| Payments
					.icons
						a(ng-click="init()")
							i.fa.fa-refresh
						a(onclick="toggleTech()")
							i.fa.fa-indent
						a(onclick="$('.modal.fade').modal('show') ")
							i.fa.fa-gears
			.contentWrapper#payments(ng-controller="paymentsCtrl")
				.row
					.col-md-5.clearfix#business
							include checkout
					.col-md-7#tech
							include tech
			include modal
			script init()